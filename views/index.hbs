<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<h1>My Viking Portfolio</h1>

{{#each projects as |project|}}
  <div class="project">

    <div class="project-name">
      <div = "/projects/{{project.name}}">
        {{project.name}}
      </div>
    </div>

    <div class="project-page">
      <a href="/projects/{{project.page}}">
         {{project.page}}
       </a>
    </div>

    <div class="project-domains">
      {{#each project.domains as |domain|}}
        <div class="project-domain">
          {{domain}}
        </div>
      {{/each}}
    </div>
  </div>
{{/each}}
